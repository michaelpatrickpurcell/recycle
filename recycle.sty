\tikzset{guidecross/.pic={
	\draw[#1] (-\bleed,0) -- (\bleed,0);
	\draw[#1] (0, -\bleed) -- (0, \bleed);
}}

\tikzset{cutguide/.pic={
	\pic () at (-\cardwidth/2, \cardheight/2) {guidecross={#1}};
	\pic () at (\cardwidth/2, \cardheight/2) {guidecross={#1}};
	\pic () at (-\cardwidth/2, -\cardheight/2) {guidecross={#1}};
	\pic () at (\cardwidth/2, -\cardheight/2) {guidecross={#1}};
}}

\tikzset{star/.pic={
