\tikzset{cardbackpattern/.pic={
	\foreach \i in {-1.25, -1.0,...,1.25}
		\foreach \j in {-1.75, -1.5, ..., 1.75}
			\draw[very thick, #1] (\i,\j) circle (0.105);

	\foreach \i in {-1.375, -1.125, -0.875,...,1.125, 1.375}
		\foreach \j in {-1.875, -1.625, ..., 1.875}
			\draw[very thick, #1] (\i,\j) circle (0.105);
}}

